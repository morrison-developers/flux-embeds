{
  "name": "flux-embeds",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "gen:feature": "node scripts/gen-feature.mjs",
    "prisma:generate": "DATABASE_URL=${DATABASE_URL:-${POSTGRES_URL:-$POSTGRES_PRISMA_URL}} DIRECT_URL=${DIRECT_URL:-${POSTGRES_URL_NON_POOLING:-${POSTGRES_URL:-$POSTGRES_PRISMA_URL}}} prisma generate",
    "prisma:migrate": "DATABASE_URL=${DATABASE_URL:-${POSTGRES_URL:-$POSTGRES_PRISMA_URL}} DIRECT_URL=${DIRECT_URL:-${POSTGRES_URL_NON_POOLING:-${POSTGRES_URL:-$POSTGRES_PRISMA_URL}}} prisma migrate deploy",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@prisma/client": "^6.3.1",
    "@tsparticles/react": "^3.0.0",
    "@tsparticles/slim": "^3.9.1",
    "next": "16.1.6",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@vitest/coverage-v8": "^3.0.6",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "16.1.6",
    "prisma": "^6.3.1",
    "tailwindcss": "^4",
    "typescript": "^5",
    "vitest": "^3.0.6"
  }
}
